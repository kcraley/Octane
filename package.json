{
  "name": "discord-bot",
  "version": "0.0.1",
  "description": "A homegrown Discord chat bot for development and learing purposes",
  "author": "Keith Craley <Kcral21@gmail.com>",
  "scripts": {
    "dev": "nodemon ./src/discordie.js --exec babel-node",
    "lint": "eslint --output-file ./eslint.report --format json --ext js ./src/",
    "security-check": "nsp check",
    "start": "babel-node ./",
    "start:discordie": "babel-node ./src/discordie.js",
    "start:discord.io": "babel-node ./src/discordio.js",
    "build": "babel ./src"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "discord.io": "github:woor/discord.io#gateway_v6",
    "discordie": "^0.11.0",
    "glob": "^7.1.2",
    "moment": "^2.20.1",
    "ramda": "^0.25.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.18.1",
    "eslint-config-busbud": "^0.3.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.1",
    "nock": "^9.1.6",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1"
  }
}
